<div class="page-header">
  <%= link_to articles_path, class: 'btn btn-default' do %>
    All Articles
  <% end %>
  <%= link_to edit_article_path(@article), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show article</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @article.title %></dd>
  <dt>Content:</dt>
  <dd><%= @article.content %></dd>
  <dt>Published at:</dt>
  <dd><%= @article.published_at %></dd>
  <dt>Is published:</dt>
  <dd><%= @article.is_published %></dd>
  <dt>User:</dt>
  <dd><%= @article.user %></dd>
</dl>
